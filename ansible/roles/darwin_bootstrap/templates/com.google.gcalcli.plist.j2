<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>com.google.gcalcli</string>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/bin/get_next_calendar_event</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>WorkingDirectory</key>
	<string>/usr/local</string>
	<key>StandardOutPath</key>
	<string>/Users/{{ ansible_env.USER }}/.gcalcli_next_event</string>
	<key>StandardErrorPath</key>
	<string>/var/log/get_next_calendar_event.log</string>
	<key>StartInterval</key>
	<integer>30</integer>
</dict>
</plist>
